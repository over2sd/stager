USE stager;
CREATE TABLE IF NOT EXISTS member ( mid INT(8) UNSIGNED PRIMARY KEY AUTO_INCREMENT, famname VARCHAR(30) NOT NULL, givname VARCHAR(25) NOT NULL, hphone CHAR(10), mphone CHAR(10), email VARCHAR(254), age CHAR(3), address VARCHAR(255), city VARCHAR(100), state CHAR(3), zip VARCHAR(10), notes TEXT, dob DATE, memtype CHAR(2));
CREATE TABLE IF NOT EXISTS cv (rid INT(12) UNSIGNED PRIMARY KEY AUTO_INCREMENT, mid INT(8) UNSIGNED, work INT(12) UNSIGNED NOT NULL, role VARCHAR(30) NOT NULL, year INT(4) UNSIGNED, month INT(2) UNSIGNED, troupe INT(8) UNSIGNED, rtype INT(4) UNSIGNED);
CREATE TABLE IF NOT EXISTS troupe (tid INT(8) UNSIGNED PRIMARY KEY AUTO_INCREMENT, tname VARCHAR(30) NOT NULL, notes TEXT);
CREATE TABLE IF NOT EXISTS work (wid INT(12) UNSIGNED PRIMARY KEY AUTO_INCREMENT, sname VARCHAR(100) NOT NULL);
